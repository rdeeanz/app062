                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "SELECT ROW_NUMBER() OVER (ORDER BY asset_categories) as no, asset_categories as nama_aktiva, SUM(kebutuhan_dana) / 1000000 as kebutuhan_dana, SUM(rkap) / 1000000 as rkap_awal, SUM(rkap) * 0.85 / 1000000 as rkap_transfer, SUM(rkap_oktober) / 1000000 as okt_rkap, SUM(realisasi_oktober) / 1000000 as okt_realisasi, SUM(rkap_januari + rkap_februari + rkap_maret + rkap_april + rkap_mei + rkap_juni + rkap_juli + rkap_agustus + rkap_september + rkap_oktober) / 1000000 as sd_okt_rkap, SUM(realisasi_januari + realisasi_februari + realisasi_maret + realisasi_april + realisasi_mei + realisasi_juni + realisasi_juli + realisasi_agustus + realisasi_september + realisasi_oktober) / 1000000 as sd_okt_realisasi, SUM(rkap) * 0.9 / 1000000 as taksasi, CASE WHEN SUM(rkap) > 0 THEN SUM(realisasi_januari + realisasi_februari + realisasi_maret + realisasi_april + realisasi_mei + realisasi_juni + realisasi_juli + realisasi_agustus + realisasi_september + realisasi_oktober) / SUM(rkap) ELSE 0 END as capaian FROM project_investasi GROUP BY asset_categories ORDER BY asset_categories;",
