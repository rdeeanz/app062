                    "rawSql": "SELECT \n  'Januari' as bulan, 1 as urutan, SUM(rkap_januari) as \"RKAP\", SUM(realisasi_januari) as \"Realisasi\" FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Februari', 2, SUM(rkap_februari), SUM(realisasi_februari) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Maret', 3, SUM(rkap_maret), SUM(realisasi_maret) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'April', 4, SUM(rkap_april), SUM(realisasi_april) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Mei', 5, SUM(rkap_mei), SUM(realisasi_mei) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Juni', 6, SUM(rkap_juni), SUM(realisasi_juni) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Juli', 7, SUM(rkap_juli), SUM(realisasi_juli) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Agustus', 8, SUM(rkap_agustus), SUM(realisasi_agustus) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'September', 9, SUM(rkap_september), SUM(realisasi_september) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Oktober', 10, SUM(rkap_oktober), SUM(realisasi_oktober) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'November', 11, SUM(rkap_november), SUM(realisasi_november) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nUNION ALL SELECT 'Desember', 12, SUM(rkap_desember), SUM(realisasi_desember) FROM project_investasi WHERE klaster_regional LIKE '%Regional%'\nORDER BY urutan;",
